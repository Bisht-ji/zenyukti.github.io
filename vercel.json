{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": ":sub.zenyukti.in"
        }
      ],
      "destination": "https://zenyukti.in/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.zenyukti.in"
        }
      ],
      "destination": "https://zenyukti.in/:path*",
      "permanent": true
    }
  ]
}
